    "type-system:migrate": "npm run migrate-types && npm run fix-imports",
    "type-system:validate": "npm run type-check && npm run validate-types",
    "type-check:strict": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --config jest.coverage.config.js --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "test:unit": "jest src/lib/__tests__/ --testTimeout=30000",
    "build:export": "next build",
    "build:validate": "node scripts/validate-build.js",
    "build:verified": "npm run build:export && npm run build:validate",
    "optimize-images": "node scripts/smart-image-optimizer.js",
    "optimize-images:force": "node scripts/smart-image-optimizer.js --force",
    "optimize-images:report": "node scripts/smart-image-optimizer.js --report",
    "build:smart": "npm run optimize-images && npm run build:export",
    "deploy:git": "git add -A && git commit -m \"Deploy update\" && git push",
    "deploy:files": "scp -r out/* root@143.42.189.57:/var/www/shop/",
    "deploy": "npm run build:verified && npm run deploy:git && npm run deploy:files",
    "deploy:full": "npm run lint && npm run type-check && npm run deploy",
    "deploy:safe": "npm run lint && npm run type-check && npm run build:verified && npm run deploy:git && npm run deploy:files",
    "analyze": "ANALYZE=true npm run build",
    "image:report": "npm run optimize-images:report",
    "image:clean": "rm -rf public/optimized .image-cache.json",
    "setup:deployment": "node scripts/setup-deployment.js",
    "health:check": "node scripts/server-health-check.js",
    "deploy:github": "echo 'Use GitHub Actions: gh workflow run deploy-production.yml'",
    "deploy:manual": "npm run build:verified && npm run deploy:git && npm run deploy:files"
  },
